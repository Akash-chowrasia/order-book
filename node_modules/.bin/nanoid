#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/knife/workspace/personal/order-book-backend/node_modules/.pnpm/nanoid@3.1.22/node_modules/nanoid/bin/nanoid.cjs/node_modules:/home/knife/workspace/personal/order-book-backend/node_modules/.pnpm/nanoid@3.1.22/node_modules/nanoid/bin/node_modules:/home/knife/workspace/personal/order-book-backend/node_modules/.pnpm/nanoid@3.1.22/node_modules/nanoid/node_modules:/home/knife/workspace/personal/order-book-backend/node_modules/.pnpm/nanoid@3.1.22/node_modules:/home/knife/workspace/personal/order-book-backend/node_modules/.pnpm/node_modules:/home/knife/workspace/personal/order-book-backend/node_modules:/home/knife/workspace/personal/node_modules:/home/knife/workspace/node_modules:/home/knife/node_modules:/home/node_modules:/node_modules:/home/knife/workspace/personal/order-book-backend/node_modules/nanoid/bin/nanoid.cjs/node_modules:/home/knife/workspace/personal/order-book-backend/node_modules/nanoid/bin/node_modules:/home/knife/workspace/personal/order-book-backend/node_modules/nanoid/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nanoid/bin/nanoid.cjs" "$@"
else 
  exec node  "$basedir/../nanoid/bin/nanoid.cjs" "$@"
fi
